{
    "name": "semver/github-action",
    "description": "Create next semver for composer.json with github actions",
    "keywords": ["github-actions","semver"],
    "license": "MIT",
    "type": "project",
    "version": "1.32.0",
    "require": {
        "php": ">=7.1.3"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "discard-changes": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
